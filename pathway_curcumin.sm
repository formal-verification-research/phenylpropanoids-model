//DCS - Diketide-CoA Synthase
const int MW_DCS = 42047; //Da
const double E_DCS;// = (50/MW_DCS)*1000; //uM
const double S50_DCS_fcoa = 10;//46; //
const double Kcat_DCS_fcoa = 10;//0.02;
const double n_DCS_fcoa = 1.8;
formula rate_DCS_fcoa = (Kcat_DCS_fcoa*E_DCS*pow(feruloyl_CoA,n_DCS_fcoa)) / (pow(S50_DCS_fcoa,n_DCS_fcoa) + pow(feruloyl_CoA,n_DCS_fcoa)); //Hill Equation

