const double Kcat_TAL = 155; //1/s
formula rate_TAL_tyr = (Kcat_TAL*E_TAL*L_tyrosine)/(Km_TAL+L_tyrosine);

//4CL - p-Coumaroyl-CoA ligase 6.2.1.12
const int MW_4CL = 62559; //Da
const double E_4CL;// = (gen_E/MW_4CL)*1000; //uM
const double Km_4CL_pca = 26;//25.1; //uM, substrate p-coumaric acid
const double Kcat_4CL_pca = 88.68;//16.3279; //1/s, substrate p-coumaric acid
const double Km_4CL_fa = 27;//56.7; //uM, substrate ferulic acid
const double Kcat_4CL_fa = 126;//7.4758; //1/s, substrate ferulic acid
const double Km_4CL_ca = 44;//11; //uM, substrate caffeic acid
const double Kcat_4CL_ca = 31.4;//0.27*Kcat_4CL_pca; //1/s, substrate caffeic acid, literature reports 27% of p-coumaric acid
formula rate_4CL_pca = (Kcat_4CL_pca*E_4CL*p_coumaric_acid)/(Km_4CL_pca+p_coumaric_acid);
formula rate_4CL_fa = (Kcat_4CL_fa*E_4CL*ferulic_acid)/(Km_4CL_fa+ferulic_acid);
formula rate_4CL_ca = (Kcat_4CL_ca*E_4CL*caffeic_acid)/(Km_4CL_ca+caffeic_acid);

//C3H - p-Coumarate 3-Hydroxylase
const int MW_C3H = 57055; //Da
const double E_C3H;// = (50/MW_C3H)*1000; //uM
const double Km_C3H_pca = 143.03; //uM, substrate p-coumaric acid
const double Kcat_C3H_pca = 0.0347; //1/s, substrate p-coumaric acid
const double Km_C3H_pcoa = 27;//143.03; //uM, substrate p-coumaroyl CoA
const double Kcat_C3H_pcoa = 126;//0.0347; //1/s, substrate p-coumaroyl CoA
formula rate_C3H_pca = (Kcat_C3H_pca*E_C3H*p_coumaric_acid) / (Km_C3H_pca+p_coumaric_acid);
formula rate_C3H_pcoa = (Kcat_C3H_pcoa*E_C3H*p_coumaroyl_CoA) / (Km_C3H_pcoa+p_coumaroyl_CoA);

//COMT - Caffeic Acid 3-O-methyltransferase
const int MW_COMT = 24602; //Da
const double E_COMT;// = (gen_E/MW_COMT)*1000; //uM
const double Km_COMT_ca = 24;//59.5; //uM, substrate caffeic acid
const double Kcat_COMT_ca = .62;//0.145152; //1/s, substrate caffeic acid
const double Km_COMT_ccoa = 26; //uM, substrate caffeoyl CoA
const double Kcat_COMT_ccoa = 0.145152; //1/s, substrate caffeoyl CoA, #TODO: MAJOR ASSUMPTION, CHECK
formula rate_COMT_ca = (Kcat_COMT_ca*E_COMT*caffeic_acid) / (Km_COMT_ca+caffeic_acid);
formula rate_COMT_ccoa = (Kcat_COMT_ccoa*E_COMT*caffeoyl_CoA) / (Km_COMT_ccoa+caffeoyl_CoA);

